<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context="com.example.tony.tonydemo.View.AnimFrameActivity">


    <com.example.tony.tonydemo.widget.SearchView
        android:id="@+id/searchView"
        android:layout_width="368dp"
        android:layout_height="551dp"
        android:fontFamily="serif"
        android:gravity="center_vertical"
        android:hint="输入搜索的信息"
        android:padding="6dp"
        android:text="北京欢迎你"
        android:textColor="#B5B5B5"
        android:textSize="16sp"
        tools:layout_editor_absoluteY="8dp"
        tools:layout_editor_absoluteX="8dp" />

    <ImageView
        android:id="@+id/animView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/dongzuo"
        tools:layout_conversion_absoluteX="16dp"
        tools:layout_conversion_absoluteY="42dp"
        tools:layout_conversion_absoluteWidth="150dp"
        tools:layout_conversion_absoluteHeight="105dp"
        tools:layout_editor_absoluteY="42dp"
        tools:layout_editor_absoluteX="16dp" />

    <com.example.tony.tonydemo.widget.DongzuoView
        android:id="@+id/animViewbig"
        android:layout_width="352dp"
        android:layout_height="200dp"
        tools:layout_conversion_absoluteX="16dp"
        tools:layout_conversion_absoluteY="147dp"
        tools:layout_conversion_absoluteWidth="352dp"
        tools:layout_conversion_absoluteHeight="200dp"
        tools:layout_editor_absoluteY="147dp"
        tools:layout_editor_absoluteX="16dp" />

    <com.example.tony.tonydemo.widget.ItemView
        android:id="@+id/item"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:imgsrc="@mipmap/zly"
        app:itemtext="照片"
        tools:layout_conversion_absoluteX="16dp"
        tools:layout_conversion_absoluteY="347dp"
        tools:layout_conversion_absoluteWidth="100dp"
        tools:layout_conversion_absoluteHeight="116dp"
        tools:layout_editor_absoluteY="347dp"
        tools:layout_editor_absoluteX="16dp">

    </com.example.tony.tonydemo.widget.ItemView>

    <com.example.tony.tonydemo.widget.NumProgress
        android:id="@+id/progressNum"
        android:layout_width="200dp"
        android:layout_height="200dp"
        tools:layout_conversion_absoluteX="16dp"
        tools:layout_conversion_absoluteY="463dp"
        tools:layout_conversion_absoluteWidth="200dp"
        tools:layout_conversion_absoluteHeight="200dp"
        tools:layout_editor_absoluteY="463dp"
        tools:layout_editor_absoluteX="16dp" />

    <com.example.tony.tonydemo.widget.ClockView
        android:id="@+id/clock"
        android:layout_width="200dp"
        android:layout_height="200dp"
        tools:layout_conversion_absoluteX="16dp"
        tools:layout_conversion_absoluteY="663dp"
        tools:layout_conversion_absoluteWidth="200dp"
        tools:layout_conversion_absoluteHeight="200dp"
        tools:layout_editor_absoluteY="663dp"
        tools:layout_editor_absoluteX="16dp" />


</>
